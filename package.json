    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "gatsby": "^2.29.2",
    "gatsby-core-utils": "^1.7.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
